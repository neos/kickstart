{
    "name": "neos/kickstart",
    "type": "neos-framework",
    "description": "A simple generator for controller and views.",
    "license": "MIT",
    "require": {
        "neos/flow": "*",
        "neos/utility-arrays": "*"
    },
    "autoload": {
        "psr-4": {
            "Neos\\Kickstart\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neos\\Kickstart\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3/kickstart": "self.version"
    },
    "extra": {
        "neos": {
            "package-key": "Neos.Kickstart"
        }
    }
}
